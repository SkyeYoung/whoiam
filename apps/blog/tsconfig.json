{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "verbatimModuleSyntax": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "blog/assets/*": ["./src/assets/blog/*"],
      "friends/assets/*": ["./src/assets/friends/*"],
    },
    "types": ["unplugin-icons/types/react"]
  },
}
